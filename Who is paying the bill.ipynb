   "source": [
    "import random\n",
    "\n",
    "name_str = input(\"Give me a list of names separated by a comma: \")\n",
    "name = name_str.split(\",\")\n",
    "name_item = len(name)\n",
    "random_choice = random.randint(0, name_item-1)\n",
    "who_is_paying_the_bill = name[random_choice]\n",
    "print(who_is_paying_the_bill + \" is going to pay the bill\")"
   ]
